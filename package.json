{
  "name": "stranger_pulse",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "env:prod": "shx cp .env.production .env",
    "env:exp": "shx cp .env.experiment .env",
    "env:admin": "shx cp .env.admin .env",
    "start:prod": "npm run env:prod && npx expo start",
    "start:exp": "npm run env:exp && npx expo start",
    "start:admin": "npm run env:admin && npx expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:admin": "shx cp .env.admin .env && expo build:android",
    "build:public": "shx cp .env.public .env && expo build:android",
    "build:both": "npm run build:admin && npm run build:public"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-form": "^1.27.1",
    "@tanstack/react-query": "^5.90.5",
    "dayjs": "^1.11.18",
    "expo": "~54.0.15",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "fast-xml-parser": "^5.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "shx": "^0.4.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
